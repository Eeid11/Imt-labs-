#include <stdio.h>

int main() {
    float x, sum = 0, av = 0;
    printf("inter the 10 number\n");
    for (int i = 0; i < 10; i++)
    {
        printf("Number%d:",i);
        scanf("%f",&x);
        sum+=x;
    }
    printf("the sume of the 10 number is :%.2f\n", sum);
    printf("the average is               :%.2f", av = sum / 10);

    return 0;
}
